  "paths": {
    "/GetEcus": {
      "get": {
        "tags": [
          "FlashingStation",
          "EcuManagement"
        ],
        "summary": "This endpoint allows flashing station to upload serial number and corresponding get Ecu keys.",
        "description": "SBS validates that the serial number is provided, is 22 bytes, and is alphanumeric. #SBS converts serial number to hex. #SBS protects data and stores it in database. #SBS returns generatedKey.",
        "responses": {
          "201": {
            "description": "Successfully updated",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          },
          "400": {
            "description": "The provided serial number {serialNumber} should be 22 bytes. # The provided serial number should be hex."
          },
          "409": {
            "description": "The provided serial number already exists in database."
          }
        }
      }
    },
